<h2>Available drones</h2>
<ul>
{{#each dronesArray}}
   <li> {{this.name}} </li>
   <a href="/drones/{{this._id}}/edit"><button type="submit" class="btn btn-primary">Edit</button></a>
   <form method="POST" action="/drones/{{this._id}}/delete"><button type="submit" class="btn btn-primary">Delete</button></form>
{{/each}}
</ul>